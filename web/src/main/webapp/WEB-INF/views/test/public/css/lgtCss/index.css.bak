#main-menu a{
	line-height:41px;
	color:#434343;
	
	float: left;
}

#main-menu li{
	margin-left: 10px;
}

#main-menu img{
    float: left;
        
}

.collapse li a span{
	color:white;
	
}



.nav-list.collapse li a{
	background-image:url(__IMAGE__/leftbuttoa2.png);
	background-size:100% 100%;
}

span.fa.fa-caret-right {
