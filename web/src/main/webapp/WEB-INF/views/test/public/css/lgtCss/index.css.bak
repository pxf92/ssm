	background-color:rgba(0 ,0, 0, 0);
}

.sidebar-nav .nav-header {
    color: white;
    border: none;
    text-align: center;
    font-size: 17px;
    padding: 5px 10% 5px 0px;
    letter-spacing: 8px;
	text-decoration: none;
}
